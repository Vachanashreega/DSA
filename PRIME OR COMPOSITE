#include <stdio.h>
int main() 
{
    int num = 7, Prime = 1;
    for (int i = 2; i <= num / 2; ++i) 
    {
        if (num % i == 0)
        {
            isPrime = 0;
            break;
        }
    }
    if (Prime)
        printf("%d is prime.\n", num);
    else
        printf("%d is composite.\n", num);
    return 0;
}

